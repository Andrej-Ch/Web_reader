<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="app.controller.view.WebsiteOverviewController">
  <TableView fx:id="websiteTable" prefHeight="450.0" prefWidth="800.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    <columnResizePolicy>
      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
    </columnResizePolicy>
    <columns>
      <TableColumn prefWidth="75.0" text="Name" fx:id="siteNameColumn" />
      <TableColumn minWidth="50.0" prefWidth="75.0" text="URL" fx:id="threadURLColumn" />
      <TableColumn prefWidth="75.0" text="Mood" fx:id="threadMoodColumn" />
    </columns>
  </TableView>
  <Button mnemonicParsing="false" onAction="#handleDeleteWebsite" text="Delete" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="20.0" />
</AnchorPane>
